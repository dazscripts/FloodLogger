function StartAntiLag()
    local CurrentMap = Workspace.Multiplayer.Map
    for _,v in pairs(CurrentMap:GetDescendants()) do 
        if v:IsA("BasePart") then
            v.Transparency = 1
            if v.CanCollide == false then
                v:Destroy()
            end
        elseif v:IsA("Texture") then
            v:Destroy()
        end
    end
    for _,v in pairs(game.Lighting:GetChildren()) do 
        if v.Name ~= "Fade_Blur" then
            v:Destroy()
        end
    end
end

function CreateAntiLagPart()
    local plr = game.Players.LocalPlayer

    local part = Instance.new("Part")
    part.Size = Vector3.new(40,40,40)
    part.Anchored = false
    part.CanCollide = false
    part.Massless = true
    part.Parent = plr.Character
    part.Transparency = 0.4
    part.Name = "AntiLagObject"
    part.CFrame = plr.Character.HumanoidRootPart.CFrame
    
    local weld = Instance.new("WeldConstraint")
    weld.Part0 = plr.Character.HumanoidRootPart
    weld.Part1 = part
    weld.Parent = plr.Character.HumanoidRootPart 

    part.Touched:Connect(function(hit)
        if hit:IsA("BasePart") then
            hit.Transparency = 0.8
        end
    end)

    part.TouchEnded:Connect(function(hit)
        if hit:IsA("BasePart") then
            hit.Transparency = 1
        end
    end)
end